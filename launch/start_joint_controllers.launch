<launch>
    <!-- Start dual_motor joint controller -->
    <rosparam file="$(find ros_gripper)/config/joint_params.yaml" command="load"/>
    <node name="wrist_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port ttlUSB0_port
                wrist_controller"
          output="screen"/>

    <node name="dual_grip_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port ttlUSB0_port
                dual_grip_controller"
          output="screen"/>
</launch>
